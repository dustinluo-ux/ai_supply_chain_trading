# Multi-Source Alpha Pipeline - Environment Variables (sync with .env.example)
# Copy to .env and fill in your API keys

# Tiingo API Key (for news data and ticker tagging)
TIINGO_API_KEY=your_tiingo_api_key_here

# Google/Gemini API Key (for sentiment and legacy supply chain LLM)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_google_api_key_here

# Marketaux API Key (for news)
MARKETAUX_API_KEY=your_marketaux_api_key_here

# EODHD API Key (for missing price backfill and global news history)
EODHD_API_KEY=your_eodhd_api_key_here

# Hugging Face Token (for FNSPID dataset)
HF_TOKEN=your_huggingface_token_here
HUGGINGFACE_HUB_TOKEN=your_huggingface_token_here

# Optional: News API, Alpaca, IBKR
NEWS_API_KEY=your_newsapi_key_here
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# IBKR (live trading)
IBKR_HOST=127.0.0.1
IBKR_PORT=7497
IBKR_CLIENT_ID_START=99

# Storage
STORAGE_MODE=LOCAL
DATA_DIR=C:/trading_data
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET_NAME=
