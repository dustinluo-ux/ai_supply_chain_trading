# Multi-Source Alpha Pipeline - Environment Variables
# Copy this file to .env and fill in your API keys

# Tiingo API Key (for news data and ticker tagging)
# Get key from: https://api.tiingo.com/
TIINGO_API_KEY=your_tiingo_api_key_here

# Google/Gemini API Key (for sentiment scoring and supply chain analysis)
# Get key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_google_api_key_here  # Legacy alias (optional)

# Marketaux API Key (for highly relevant English news)
# Get key from: https://www.marketaux.com/
MARKETAUX_API_KEY=your_marketaux_api_key_here

# EODHD API Key (for missing price backfill and global news history)
# Get key from: https://eodhd.com/
EODHD_API_KEY=your_eodhd_api_key_here

# Hugging Face Token (for downloading FNSPID dataset)
# Get token from: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here
HUGGINGFACE_HUB_TOKEN=your_huggingface_token_here

# News API (optional - for additional news sources)
# Get key from: https://newsapi.org/register
NEWS_API_KEY=your_newsapi_key_here

# Alpaca Trading API (optional - for paper trading)
# Get key from: https://alpaca.markets/
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# IBKR Configuration (optional - for live trading)
IBKR_HOST=127.0.0.1
IBKR_PORT=7497
IBKR_CLIENT_ID_START=99

# Storage
STORAGE_MODE=LOCAL
DATA_DIR=C:/trading_data
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET_NAME=
